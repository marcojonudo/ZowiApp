<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:id="@+id/seeds_images_container"
    android:background="@drawable/unit_1_seeds_seed_container"
    app:layout_constraintDimensionRatio="4:1"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintBottom_toTopOf="@+id/seeds_guideline"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    android:layout_marginStart="@dimen/seeds_container_horizontal_margin"
    android:layout_marginEnd="@dimen/seeds_container_horizontal_margin"
    android:padding="@dimen/seeds_container_padding">

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/seed_1"
        app:layout_constraintTop_toTopOf="@+id/seeds_template_guideline"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/seed_2"
        android:layout_margin="@dimen/seed_margin"/>

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/seed_2"
        app:layout_constraintTop_toTopOf="@+id/seeds_template_guideline"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/seed_1"
        app:layout_constraintRight_toLeftOf="@+id/seed_3"
        android:layout_margin="@dimen/seed_margin"/>

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/seed_3"
        app:layout_constraintTop_toTopOf="@+id/seeds_template_guideline"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/seed_2"
        app:layout_constraintRight_toLeftOf="@+id/seed_4"
        android:layout_margin="@dimen/seed_margin"/>

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/seed_4"
        app:layout_constraintTop_toTopOf="@+id/seeds_template_guideline"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/seed_3"
        app:layout_constraintRight_toLeftOf="@+id/seed_5"
        android:layout_margin="@dimen/seed_margin"/>

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/seed_5"
        app:layout_constraintTop_toBottomOf="@+id/seeds_template_guideline"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/seed_4"
        app:layout_constraintRight_toLeftOf="@+id/seed_6"
        android:layout_margin="@dimen/seed_margin"/>

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/seed_6"
        app:layout_constraintTop_toBottomOf="@+id/seeds_template_guideline"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/seed_5"
        app:layout_constraintRight_toLeftOf="@+id/seed_7"
        android:layout_margin="@dimen/seed_margin"/>

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/seed_7"
        app:layout_constraintTop_toBottomOf="@+id/seeds_template_guideline"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/seed_6"
        app:layout_constraintRight_toLeftOf="@+id/seed_8"
        android:layout_margin="@dimen/seed_margin"/>

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/seed_8"
        app:layout_constraintTop_toBottomOf="@+id/seeds_template_guideline"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/seed_7"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_margin="@dimen/seed_margin"/>

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/seeds_template_guideline"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.5"/>

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/seed_9"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/seeds_template_guideline"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/seed_10"
        android:layout_margin="@dimen/seed_margin"/>

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/seed_10"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/seeds_template_guideline"
        app:layout_constraintLeft_toRightOf="@+id/seed_9"
        app:layout_constraintRight_toLeftOf="@+id/seed_11"
        android:layout_margin="@dimen/seed_margin"/>

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/seed_11"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/seeds_template_guideline"
        app:layout_constraintLeft_toRightOf="@+id/seed_10"
        app:layout_constraintRight_toLeftOf="@+id/seed_12"
        android:layout_margin="@dimen/seed_margin"/>

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/seed_12"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/seeds_template_guideline"
        app:layout_constraintLeft_toRightOf="@+id/seed_11"
        app:layout_constraintRight_toLeftOf="@+id/seed_13"
        android:layout_margin="@dimen/seed_margin"/>

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/seed_13"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/seeds_template_guideline"
        app:layout_constraintLeft_toRightOf="@id/seed_12"
        app:layout_constraintRight_toLeftOf="@+id/seed_14"
        android:layout_margin="@dimen/seed_margin"/>

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/seed_14"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/seeds_template_guideline"
        app:layout_constraintLeft_toRightOf="@+id/seed_13"
        app:layout_constraintRight_toLeftOf="@+id/seed_15"
        android:layout_margin="@dimen/seed_margin"/>

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/seed_15"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/seeds_template_guideline"
        app:layout_constraintLeft_toRightOf="@+id/seed_14"
        app:layout_constraintRight_toLeftOf="@+id/seed_16"
        android:layout_margin="@dimen/seed_margin"/>

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/seed_16"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/seeds_template_guideline"
        app:layout_constraintLeft_toRightOf="@+id/seed_15"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_margin="@dimen/seed_margin"/>

</android.support.constraint.ConstraintLayout>